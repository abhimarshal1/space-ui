<table class="table table-sm">
    <thead>
        <tr>
            {{#each heads as |h index|}}
                <th scope="col">{{h.title}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
        {{#each data as |d index|}}
            <tr>
                {{#each-in d as |key value|}}
                    <td>{{{value}}}</td>
                {{/each-in}}
            </tr>
        {{/each}}
    </tbody>
</table>